{
	"name": "cashcompass",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"docker:build": "docker build . -t ericdudley/cashcompass",
		"docker:run": "docker run -p 8080:80 -d ericdudley/cashcompass",
		"docker:login": "docker login",
		"docker:stop": "docker stop $(docker ps -q --filter ancestor=ericdudley/cashcompass) || true",
		"docker:clean": "docker rmi ericdudley/cashcompass || true",
		"docker:full": "npm run docker:clean && npm run docker:build && npm run docker:run",
		"docker:restart": "npm run docker:stop && npm run docker:run",
		"docker:publish": "node scripts/docker-publish.js",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"tauri": "tauri"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.321",
		"@magidoc/plugin-svelte-marked": "^6.2.0",
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@tauri-apps/cli": "^2.4.0",
		"@types/d3-scale": "^4.0.9",
		"@types/eslint": "^9.6.1",
		"@types/lodash-es": "^4.17.12",
		"@types/marked": "6.0.0",
		"@types/papaparse": "^5.3.15",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.21",
		"daisyui": "^5.0.9",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.4.0",
		"globals": "^16.0.0",
		"install": "^0.13.0",
		"lodash-es": "^4.17.21",
		"marked": "^15.0.7",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "5.25.3",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.0.17",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.28.0",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.2.3",
		"vitest": "^3.0.9"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/postcss": "^4.0.17",
		"@tailwindcss/vite": "^4.0.17",
		"currency.js": "^2.0.4",
		"d3-scale": "^4.0.2",
		"date-fns": "^4.1.0",
		"dexie": "^4.0.11",
		"dexie-cloud-addon": "^4.0.11",
		"flowbite": "^3.1.2",
		"papaparse": "^5.5.2",
		"rxjs": "^7.8.2",
		"uuid": "^11.1.0"
	}
}
